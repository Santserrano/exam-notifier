generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "windows"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model MesaDeExamen {
  id          Int      @id @default(autoincrement())
  profesor    String
  vocal       String
  carrera     String
  materia     String
  fecha       DateTime
  descripcion String
  cargo       String
  verification Boolean
  createdAt   DateTime @default(now())
}