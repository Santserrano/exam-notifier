{
  "name": "@exam-notifier/remix-app",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "cross-env PORT=3000 remix dev --manual",
    "dev:test": "cross-env PORT=3000 NODE_ENV=test REMIX_CONFIG_FILE=remix.env.test.js remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test playwright test",
    "test:e2e:ui": "cross-env NODE_ENV=test playwright test --ui",
    "test:e2e:debug": "cross-env NODE_ENV=test playwright test --debug",
    "test:server": "cross-env PORT=3000 NODE_ENV=test REMIX_CONFIG_FILE=remix.env.test.js remix dev --manual",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "@clerk/backend": "^1.32.1",
    "@clerk/remix": "^4.7.2",
    "@clerk/themes": "^2.2.43",
    "@exam-notifier/database": "workspace:*",
    "@exam-notifier/internal-nobuild": "workspace:*",
    "@exam-notifier/ui": "workspace:*",
    "@remix-run/css-bundle": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "isbot": "^4.1.0",
    "lucide-react": "^0.510.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@exam-notifier/eslint-config": "workspace:*",
    "@exam-notifier/tsconfig": "workspace:*",
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.52.0",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/fs-routes": "^2.16.6",
    "@remix-run/route-config": "^2.16.6",
    "@remix-run/testing": "^2.16.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.8.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.1.2",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}