{
  "private": true,
  "name": "@exam-notifier/remix-vercel",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run clean && cross-env PRISMA_CLIENT_ENGINE_TYPE=binary prisma generate --schema=./prisma/schema.prisma && remix vite:build",
    "dev": "cross-env PORT=3002 remix vite:dev",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rimraf node_modules/.prisma"
  },
  "dependencies": {
    "@clerk/backend": "^1.32.1",
    "@clerk/remix": "^4.7.2",
    "@clerk/themes": "^2.2.43",
    "@exam-notifier/database": "workspace:*",
    "@exam-notifier/ui": "workspace:*",
    "@prisma/client": "^5.22.0",
    "@remix-run/css-bundle": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@remix-run/server-runtime": "^2.8.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/remix": "^2.8.1",
    "isbot": "^4.1.0",
    "lucide-react": "^0.510.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@exam-notifier/eslint-config": "workspace:*",
    "@exam-notifier/tsconfig": "workspace:*",
    "@remix-run/dev": "^2.8.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/source-map-support": "^0.5.10",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
